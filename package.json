    "@types/node": "^14.14.22",
    "clean-webpack-plugin": "^3.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.6.0",
